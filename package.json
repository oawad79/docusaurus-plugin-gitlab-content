{
    "name": "docusaurus-plugin-gitlab2-content",
    "version": "1.0.45",
    "description": "A Docusaurus v2 plugin that allows you to fetch content from GitLab!",
    "main": "build/index.js",
    "author": "Osama Awad",
    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "prettier": "prettier --write \"**/*.{js,ts,md,json}\"",
        "release": "npm build & git add . & git commit -m 'gitlab' & git push & npm version patch & npm publish"
    },
    "prettier": {
        "semi": false,
        "tabWidth": 4
    },
    "devDependencies": {
        "@docusaurus/types": "2.2.0",
        "@types/react": "^18.0.20",
        "@types/react-dom": "^18.0.6",
        "@types/react-helmet": "^6.1.4",
        "@types/node": "^18.17.1",
        "commander": "^9.4.0",
        "prettier": "^2.7.1",
        "typescript": "^4.8.4"
    },
    "peerDependencies": {
        "@docusaurus/core": "2.x"
    },
    "dependencies": {
        "axios": "^1.1.3",
        "picocolors": "^1.0.0",
        "pretty-ms": "^7.0.1"
    },
    "files": [
        "build/*",
        "src",
        "CHANGELOG.md"
    ],
    "directories": {
        "lib": "src"
    },
    "keywords": [
        "docusaurus",
        "v2",
        "plugin",
        "remote",
        "gitlab",
        "content"
    ],
    "engines": {
        "node": ">=16.14"
    }
}
